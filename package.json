    "url": "git+https://github.com/stopsopa/validator.git"
  },
  "keywords": [
    "validation"
  ],
  "author": "Szymon Dzia≈Çowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stopsopa/validator/issues"
  },
  "homepage": "https://github.com/stopsopa/validator#readme",
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "validator/**/*.{js,jsx}"
